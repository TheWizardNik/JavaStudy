public class Gun {
    private int ammo;

    public Gun(int ammo)
    {
        this.ammo=ammo;
    }

    public void shoot()
    {
        if (ammo>0) System.out.println("Бах");
        else System.out.println("Клац");
        ammo--;
    }
}
