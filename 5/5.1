public class Point
{
        private int x,y;

        public int getX() {
        return x;
    }

        public int getY() {
        return y;
        }

        String getPoint()
        {
            return "{"+x+";"+y+"}";
        }

        void setX(int x)
        {
            this.x = x;
        }

        void setY(int y)
        {
        this.y = y;
        }
        
        public Point (int x,int y)
        {
            this.x = x;
            this.y = y;
        }
}

