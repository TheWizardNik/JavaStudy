public class Singer {
    private String name;
    private String song;

    public Singer(String name)
    {
        this.name=name;
    }

    public Singer(String name,String song)
    {
        this.name=name;
        this.song=song;
    }

    public void singSong()
    {
        System.out.println(name+": "+song);
    }
}
