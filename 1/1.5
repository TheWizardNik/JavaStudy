public class House { 
private int floor; 

public House(int floor) 
{ 
this.floor=floor; 
} 

public String getFloor() 
{ 
if (floor%10==1&&floor!=11) 
return "Дом с "+floor+" этажом"; 
else return "Дом с "+floor+" этажами"; 
} 
}
